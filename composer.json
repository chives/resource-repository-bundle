{
    "name": "fsi/resource-repository-bundle",
    "type": "symfony-bundle",
    "description": "Resource repository bundle",
    "keywords": ["fsi", "resource", "repository", "bundle", "symfony"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "doctrine/dbal": "^2.6",
        "doctrine/doctrine-bundle": "^1.4",
        "php": ">=7.1",
        "symfony/dependency-injection": "^3.0|^4.0",
        "symfony/doctrine-bridge": "^3.0|^4.0",
        "symfony/event-dispatcher": "^3.0|^4.0",
        "symfony/form" : "^3.0|^4.0",
        "symfony/framework-bundle": "^3.0|^4.0",
        "symfony/property-access": "^3.0|^4.0",
        "symfony/validator": "^3.0|^4.0",
        "twig/twig": "^1.34|^2.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.5",
        "friendsofsymfony/ckeditor-bundle": "^1.0",
        "fsi/doctrine-extensions": "^1.0|^2.0@dev",
        "fsi/doctrine-extensions-bundle": "^1.0|^2.0@dev",
        "fsi/form-extensions-bundle": "^2.0@dev",
        "phpunit/phpunit": "^7.1",
        "phpspec/phpspec": "^4.3",
        "phpspec/prophecy": "^1.7",
        "sebastian/exporter": "^3.1.0"
    },
    "suggest": {
        "fsi/doctrine-extensions-bundle": "For using fsi_file resource type",
        "friendsofsymfony/ckeditor-bundle": "For using ckeditor resource type"
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "autoload": {
        "psr-4": { "FSi\\Bundle\\ResourceRepositoryBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\FSi\\Bundle\\ResourceRepositoryBundle\\": "spec",
            "FSi\\Bundle\\ResourceRepositoryBundle\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "1.1": "1.1-dev",
            "dev-master": "2.0-dev"
        }
    }
}
