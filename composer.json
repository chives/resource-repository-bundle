{
    "name": "fsi/resource-repository-bundle",
    "type": "symfony-bundle",
    "description": "Resource repository bundle",
    "keywords": ["fsi", "resource", "repository", "bundle", "symfony"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "doctrine/dbal": "^2.6",
        "doctrine/doctrine-bundle": "^1.4|^2.0",
        "symfony/dependency-injection": "^3.4|^4.0|^5.0",
        "symfony/doctrine-bridge": "^3.4|^4.0|^5.0",
        "symfony/form" : "^3.4|^4.0|^5.0",
        "symfony/framework-bundle": "^3.4|^4.0|^5.0",
        "symfony/property-access": "^3.4|^4.0|^5.0",
        "symfony/validator": "^3.4|^4.0|^5.0",
        "twig/twig": "^1.34|^2.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.5",
        "friendsofsymfony/ckeditor-bundle": "^1.0|^2.0",
        "fsi/doctrine-extensions-bundle": "^1.0|^2.0",
        "fsi/form-extensions-bundle": "^2.0",
        "phpunit/phpunit": "^7.1|^8.0",
        "phpspec/phpspec": "^5.0|^6.0",
        "phpstan/phpstan": "^0.12.66",
        "phpstan/phpstan-phpunit": "^0.12.15",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/phpunit-bridge": "^4.3|^5.0"
    },
    "suggest": {
        "fsi/doctrine-extensions-bundle": "For using fsi_file resource type",
        "friendsofsymfony/ckeditor-bundle": "For using ckeditor resource type"
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "autoload": {
        "psr-4": { "FSi\\Bundle\\ResourceRepositoryBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\": "spec",
            "FSi\\Bundle\\ResourceRepositoryBundle\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev",
            "2.1": "2.1-dev",
            "2.0": "2.0-dev",
            "1.1": "1.1-dev",
            "1.0": "1.0-dev"
        }
    }
}
