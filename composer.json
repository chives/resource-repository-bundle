{
    "name": "fsi/resource-repository-bundle",
    "type": "symfony-bundle",
    "description": "Resource repository bundle",
    "keywords": ["fsi", "resource", "repository", "bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": ">=5.4",
        "twig/twig": "~1.13",
        "symfony/framework-bundle": "^2.2|^3.0",
        "symfony/form" : "^2.2|^3.0",
        "symfony/validator": "^2.2|^3.0",
        "symfony/event-dispatcher": "^2.2|^3.0",
        "doctrine/doctrine-bundle": "^1.4",
        "symfony/doctrine-bridge": "^2.3|^3.0",
        "symfony/dependency-injection": "^2.6|^3.0",
        "symfony/property-access": "^2.3|^3.0"
    },
    "require-dev": {
        "doctrine/orm": "~2.3",
        "phpspec/phpspec": "^2.0",
        "fsi/doctrine-extensions-bundle": "~1.0",
        "fsi/form-extensions-bundle": "1.0.*"
    },
    "suggest": {
        "fsi/doctrine-extensions-bundle": "For using fsi_file resource type",
        "egeloen/ckeditor-bundle": "For using ckeditor resource type"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "FSi\\Bundle\\ResourceRepositoryBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
